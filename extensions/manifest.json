{
  "manifest_version": 3,
  "name": "Electron Download Sender",
  "version": "1.0",
  "description": "Send downloads to Electron Download Manager",
  "permissions": [
    "downloads",
    "notifications",
    "tabs"
  ],
  "host_permissions": [
    "http://127.0.0.1/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Send to Electron Downloader"
  }
}
